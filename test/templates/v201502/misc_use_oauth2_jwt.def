  file_contents = 'file contents'
  key_secret = 'not-secret'

  $api_config = {
    :service => {:environment => 'PRODUCTION'},
    :authentication => {
      :method => 'OAUTH2_JWT',
      :oauth2_issuer => 'not-valid@developer.gserviceaccount.com',
